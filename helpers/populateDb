const db = require('../db');
const user = require('../models/users')
const task = require('../models/tasks')


module.exports = {
  populateUsers: () => {
    new Promise((resolve, reject ) => {
      db.cypher({
        query:'CREATE (u:User) ',
        params: {

        },
      })(err,result) => {
        console.log('populating');
        if(err) reject(err);
        if(!result.length){
          console.log('couldnt create a user')
          resolve({message: 'couldnt find'});
        }else {
          resolve{result}
        }
      }
    })
  }
}
